{
  "name": "hookedin",
  "version": "0.0.1",
  "description": "hookedin wallet",
  "scripts": {
    "dev": "webpack-dev-server",
    "schema": "ts-node src/wallet/generate-schema.ts > src/wallet/schema.ts && prettier --single-quote --trailing-comma es5 --print-width 160 --write src/wallet/schema.ts",
    "build": "webpack --config webpack.config.js",
    "build-cdn": "NODE_ENV=production USE_CDN=true webpack --config webpack.config.js",
    "prettier": "prettier --single-quote --trailing-comma es5 --print-width 160 --write \"src/**/*.{ts,tsx}\"",
    "publish": "npm run build-cdn && cp -r dist/* ../hundredeyes-dist && cd ../hundredeyes-dist/ && git add -A && git c -m \"Add built files\" && git pull --rebase && git push"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/reactstrap": "^8.0.0",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "reactstrap": "^8.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1",
    "webpack-subresource-integrity": "^1.3.2"
  },
  "dependencies": {
    "@types/react-transition-group": "^2.9.2",
    "deep-equal": "^1.0.1",
    "hookedin-lib": "git+https://github.com/hookedin/hookedin-lib.git",
    "idb": "^4.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^5.2.0",
    "react-transition-group": "^4.1.1",
    "the-qr": "^2.1.3"
  },
  "browser": {
    "./dist/bip39/pbkdf2-node.js": "./dist/bip39/pbkdf2-browser.js"
  }
}
